{
	"name": "ed-assistant",
	"displayName": "ED Assistant",
	"description": "Encryption-Decryption tool for Collections-App developers on VSCode IDE",
	"version": "0.0.1",
	"publisher": "minzie7",
	"repository": {
		"type": "git",
		"url": "https://github.com/riosaurus/ed-assistant.git"
	},
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ed-assistant.encryptText",
		"onCommand:ed-assistant.decryptText",
		"onCommand:ed-assistant.openSettings"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ed-assistant.encryptText",
				"title": "Encrypt the JSON object using the saved cipher settings",
				"category": "EDA"
			},
			{
				"command": "ed-assistant.decryptText",
				"title": "Decrypt the text to formatted JSON using the saved cipher settings",
				"category": "EDA"
			},
			{
				"command": "ed-assistant.openSettings",
				"title": "Open ED Assistant settings",
				"category": "EDA"
			}
		],
		"configuration": [
			{
				"title": "ED Assistant",
				"properties": {
					"ed-assistant.encryption.initializationVector": {
						"type": "string",
						"default": "",
						"description": "Initialization Vector to initialize cipher."
					},
					"ed-assistant.encryption.secretKey": {
						"type": "string",
						"default": "",
						"description": "Secret key for encryption."
					},
					"ed-assistant.encryption.algorithm": {
						"type": "string",
						"enum": [
							"aes-128-cbc",
							"aes-128-ecb"
						],
						"default": "aes-128-cbc",
						"description": "Encryption algorithm to use."
					},
					"ed-assistant.output.openToTheSide": {
						"type": "boolean",
						"default": false,
						"description": "If set to true, output will be opened in a read-only editor to the side of the active window."
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.62.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"ts-loader": "^9.2.5",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0",
		"@vscode/test-electron": "^1.6.2"
	},
	"dependencies": {}
}
